{
    "news247": {
        "start_url": "https://www.news247.gr/",
        "regex": "^https:\\/\\/www\\.news247\\.gr\\/(kairos|ellada|politiki|kosmos|oikonomia(\\/(agora|akinita|ergasia|forologia|tourismos|xrima))?|energia|epixeiriseis|athlitika|politismos(\\/(vivlia|theatro|mousiki|cinema|texnes))?|psixagogia(\\/(tileorasi|celebrities))?|travel|paideia|ygeia)\\/?$",
        "category": "li.menu-item a::attr(href)",
        "category_name": "li.menu-item a::text",
        "articles_container": "section.category__posts",
        "article": "article.article",
        "title_selector": "h3.post__title a::text",
        "article_url": "h3.post__title a::attr(href)",
        "image_url": "figure a.clear.post_img picture source::attr(srcset)",
        "content_container": "main.single_article__main",
        "summary": "main.single_article__main p",
        "author": "span.post__author::text",
        "text": "div.paragraph p",
        "tags": "div.article__tags ul li.tag_item a::text",
        "date_selector": "span.caption.s-font::text",
        "category_selector": "a.category-link",
        "next_page": "div.load-more-container a.load_more_posts::attr(href)",
        "url_concat": false,
        "pagination": true,
        "dynamic_loading": false,
        "date_limit": "2024-01-01"
    }
  }
  